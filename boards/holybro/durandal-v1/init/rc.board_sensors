#!/bin/sh
#
# Holybro Durandal V1 specific board sensors init
#------------------------------------------------------------------------------
adc start

# SPI1 IMUs
icm20689 -s -b 1 -R 2 start
bmi088 -A -s -b 1 -R 2 start
bmi088 -G -s -b 1 -R 2 start

# SPI4 internal barometer
ms5611 -s -b 4 start

# I2C3 internal compass
ist8310 -I -b 3 -R 10 start

# External compass: standard Holybro Pixhawk GPS/compass puck (with lights, safety button, and buzzer)
ist8310 -X -b 1 -R 10 start
